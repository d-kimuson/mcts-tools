{
  "[python]": {
    "editor.tabSize": 4
  },
  "python.venvPath": ".venv",
  "python.pythonPath": ".venv/bin/python",
  "python.formatting.provider": "autopep8",
  "python.analysis.typeCheckingMode": "off",
  "mypy.executable": "${workspaceFolder}/.venv/bin/mypyls",
  "mypy.configFile": "mypy.ini",
  "mypy.targets": ["."]
}
